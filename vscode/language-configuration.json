{
    "comments": {
        "lineComment": "//"
    },
    "brackets": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "autoClosingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "surroundingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "indentationRules": {
        "increaseIndentPattern": {
            "pattern": "\\{|\\[|\\("
        },
        "decreaseIndentPattern": {
            "pattern": "\\}|\\]|\\)"
        }
    }
}
